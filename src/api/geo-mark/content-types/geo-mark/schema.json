{
  "kind": "collectionType",
  "collectionName": "geo_marks",
  "info": {
    "singularName": "geo-mark",
    "pluralName": "geo-marks",
    "displayName": "Geo Mark"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "maxLength": 500,
      "required": true
    },
    "description": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext"
    },
    "address": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "maxLength": 500
    },
    "country": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "customField",
      "required": true,
      "customField": "plugin::country-select.country"
    },
    "type": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "Point",
        "LineString",
        "Polygon",
        "MultiPoint",
        "MultiLineString",
        "MultiPolygon",
        "GeometryCollection"
      ],
      "required": true,
      "default": "Point"
    },
    "geo": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "json",
      "required": true
    },
    "bbox": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "json"
    },
    "center": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "json",
      "required": false
    }
  }
}
